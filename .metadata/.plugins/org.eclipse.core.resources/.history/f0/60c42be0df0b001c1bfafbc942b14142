import java.io.InputStream;
import java.io.InputStreamReader;
import java.util.Scanner;

public class Tester {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=1;
		int n=5;
		int x=13;
		while (a<=x) {
				
		for (int i=1;i<=n;i++) {
			for (int j=1; j<=i;j++) {
				System.out.print(a + " ");
				a++;
			}
			System.out.println();
		}

	}
	}
}
